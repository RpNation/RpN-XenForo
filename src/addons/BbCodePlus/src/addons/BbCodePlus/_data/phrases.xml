  <phrase title="custom_bb_code_example.input" version_id="1" version_string="1.0.0"><![CDATA[Username: [input=userUsername]Username[/input]
Password: [input class=userPassword type=password maxlength=12 placeholder="Password"][/input]]]></phrase>
  <phrase title="custom_bb_code_example.nobr" version_id="1" version_string="1.0.0"><![CDATA[[nobr]First line
Second line[/nobr]]]></phrase>
  <phrase title="custom_bb_code_example.script" version_id="1" version_string="1.0.0"><![CDATA[[nobr][div class=element]Element[/div]
[script=element]
setText init
[/script]
[script class=element on=mouseenter]
set p (getText)
setText "${p} mouseenter"
[/script][/nobr]]]></phrase>
