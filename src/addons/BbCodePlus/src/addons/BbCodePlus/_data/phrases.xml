  <phrase title="custom_bb_code_example.animation" version_id="8" version_string="1.1.3"><![CDATA[[animation=example]
  [keyframe=0]background-color: yellow;[/keyframe]
  [keyframe=100]background-color: blue;[/keyframe]
[/animation]]]></phrase>
  <phrase title="custom_bb_code_example.br" version_id="2" version_string="1.0.4"><![CDATA[Some text[br][/br]broken up]]></phrase>
  <phrase title="custom_bb_code_example.class" version_id="1" version_string="1.0.0"><![CDATA[[nobr][div class=element]Element[/div]
[class=element]
background-color: red;
border: solid 1px green;
[/class]
[class name=element state=hover]
background-color: blue;
border: solid 1px yellow;
[/class][/nobr]]]></phrase>
  <phrase title="custom_bb_code_example.div" version_id="1" version_string="1.0.0"><![CDATA[[div=background-color: red;]A red background[/div]]]></phrase>
